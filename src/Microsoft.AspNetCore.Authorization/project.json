{
  "version": "1.0.2",
  "description": "ASP.NET Core authorization classes.\r\nCommonly used types:\r\nMicrosoft.AspNetCore.Authorization.AllowAnonymousAttribute\r\nMicrosoft.AspNetCore.Authorization.AuthorizeAttribute",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/security"
    },
    "tags": [
      "aspnetcore",
      "authorization"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Logging.Abstractions": "1.0.2",
    "Microsoft.Extensions.Options": "1.0.2"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Security.Claims": "4.0.1"
      }
    }
  }
}