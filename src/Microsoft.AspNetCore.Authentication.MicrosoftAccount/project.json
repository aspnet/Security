{
  "version": "1.0.2",
  "description": "ASP.NET Core middleware that enables an application to support the Microsoft Account authentication workflow.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/security"
    },
    "tags": [
      "aspnetcore",
      "authentication",
      "security"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Authentication.OAuth": "1.0.2"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {}
  }
}