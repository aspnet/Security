{
  "version": "1.0.2",
  "description": "ASP.NET Core contains middleware to support Google's OpenId and OAuth 2.0 authentication workflows.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/security"
    },
    "tags": [
      "aspnetcore",
      "authentication",
      "security"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Authentication.OAuth": "1.0.2"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {}
  }
}